{
  "name": "MTechLLC/drupal-8",
  "description": "Basic Drupal 8 Install",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "web/modules/custom/*"
    },
    {
      "type": "package",
      "package": {
        "name": "enyo/dropzone",
        "version": "dev-master",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/enyo/dropzone.git",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "desandro/imagesloaded",
        "version": "dev-master",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/desandro/imagesloaded.git",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "desandro/masonry",
        "version": "dev-master",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/desandro/masonry.git",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "jackmoore/colorbox",
        "version": "dev-master",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/jackmoore/colorbox",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "~1",
    "cweagans/composer-patches": "~1",
    "desandro/imagesloaded": "*",
    "desandro/masonry": "*",
    "drupal-composer/drupal-scaffold": "~2",
    "drupal/advagg": "2.x-dev",
    "drupal/admin_toolbar": "~1",
    "drupal/audiofield": "~1",
    "drupal/bootstrap": "~3",
    "drupal/colorbox": "1.1",
    "drupal/config_installer": "~1",
    "drupal/console": "~1",
    "drupal/core": "~8",
    "drupal/easy_breadcrumb": "1.x-dev",
    "drupal/entity_browser": "1.x-dev",
    "drupal/file_browser": "1.x-dev",
    "drupal/google_analytics": "~2",
    "drupal/inline_entity_form": "1.x-dev",
    "drupal/link_attributes": "~1",
    "drupal/media_entity_audio": "1.x-dev",
    "drupal/media_entity_image": "~1",
    "drupal/migrate_plus": "~3",
    "drupal/migrate_tools": "~3",
    "drupal/ng_lightbox": "~1",
    "drupal/paragraphs": "1.x-dev",
    "drupal/pathauto": "~1",
    "drupal/redirect": "~1",
    "drupal/simple_sitemap": "2.x-dev",
    "drupal/smart_trim": "1.x-dev",
    "drupal/twig_tweak": "~1",
    "drupal/video_embed_field": "~1",
    "drush/config-extra": "~1",
    "drush/drush": "~8",
    "enyo/dropzone": "*",
    "geerlingguy/drupal-vm": "dev-master",
    "jackmoore/colorbox": "*"
  },
  "require-dev": {
    "behat/behat": "~3.0",
    "behat/mink-extension": "~2.2",
    "behat/mink-goutte-driver": "~1.2",
    "behat/mink-selenium2-driver": "~1.3",
    "drupal/drupal-extension": "~3.1",
    "jcalderonzumba/behat-phantomjs-extension": "^0.1.3",
    "jcalderonzumba/gastonjs": "^1.1@dev",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4",
    "symfony/css-selector": "2.7.*"
  },
  "replace": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config" : {
    "secure-http": false,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::buildScaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::buildScaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ]
    },
    "patches": {
      "drupal/core": {
        "Menu_links migration is not working because option parameters were not found": "https://www.drupal.org/files/issues/d6_7_d8_migration-2707387-49.patch",
        "Redirect to install.php on empty DB": "https://www.drupal.org/files/issues/728702-163.patch",
        "Inline responsive image styles,etc": "https://www.drupal.org/files/issues/allow_image_style_to_be-2061377-224.patch",
        "Categorize migrations according to their type": "https://www.drupal.org/files/issues/categorize_migrations-2711099-33.patch"
      },
      "drupal/config_installer": {
        "config_installer_install_uninstalled_profile_dependencies() assumes too much": "https://www.drupal.org/files/issues/config_installer_2729243_2.patch"
      }
    }
  }
}
